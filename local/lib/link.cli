if test -f libffmpeg*.a; then rm libffmpeg*.a; fi
if test -f  test2; then rm test2; fi
ar rcs  libffmpeg.a  *.o
strip --strip-debug libffmpeg.a
LIBS="-L/usr/lib64/gstreamer-1.0/ ./libffmpeg.a ../libavfilter/libavfilter.a ../libavformat/libavformat.a ../libavcodec/libavcodec.a ../libavutil/libavutil.a  -pthread  -lm"
gcc -I. -I.. -o test2.o  -c test2.c
gcc -o test2  test2.o $(echo $LIBS)
strip -s ./test2
