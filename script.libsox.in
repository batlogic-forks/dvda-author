if test -d "@BUILDDIR@/@MAYBE_SOX@"
then
  cp "@BUILDDIR@/sox-libs" "@BUILDDIR@/@MAYBE_SOX@/sox-libs.in" 
else
  echo "@BUILDDIR@/@MAYBE_SOX@ is not a directory"
  exit -1
fi 
cd "@MAYBE_SOX@" 

./configure @CONFIGURE_SOX_FLAGS@ --without-oggvorbis --without-mad && make -j4 -k && make install; cd - 
